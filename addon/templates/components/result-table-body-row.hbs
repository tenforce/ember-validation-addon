<td>{{add index 1}}</td>
<td> <a {{action 'onConceptClick' item}}>{{item.parameterUuid}}</a> </td>
<td>{{item.parameterType}}</td>

{{#each parameters as |param|}}
    {{#unless (eq param "type") }}
        {{#unless (eq param "uuid") }}
            <td>
                {{#if (eq param "preflabel")}}
                    <a {{action 'onConceptClick' item}}>{{get item (capitalize-parameter-name param)}}</a>
                {{/if}}
                {{#unless (eq param "preflabel")}}
                    {{get item (capitalize-parameter-name param)}}
                {{/unless}}
            </td>
        {{/unless}}
    {{/unless}}
{{/each}}
