{{#if (eql item.type "concept")}}
    <td> <a href="/concepts/{{item.parameterUuid}}">{{item.parameterUuid}}</a> </td>

{{else}}
    <td>{{item.parameterUuid}}</td>
{{/if}}
{{#if showConcept}}
    <td>{{await concept.preflabel}}</td>
{{/if}}
<td>{{item.parameterType}}</td>
{{#each parameters as |param|}}
    {{#unless (eql param "uuid") }}
        <td>
            {{get item (capitalize-parameter-name param)}}
        </td>
    {{/unless}}
{{/each}}
