<tr>
    <th>uuid</th>
    {{#if showConcept}}
        <th>concept</th>
    {{/if}}
    <th>type</th>
    {{#each parameters as |param|}}
        {{#unless (eql param "uuid") }}
            {{#unless (eql param "type") }}
                <th>{{param}}</th>
            {{/unless}}
        {{/unless}}
    {{/each}}
</tr>
