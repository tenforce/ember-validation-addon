{{#each pagedContent as |item|}}
    <tr>
        <td>{{item.id}}</td>
        <td>{{item.type}}</td>
        {{#each parameters as |param|}}
            {{#unless (eql param "uuid") }}
                <td>
                    {{get item.attributes param}}
                </td>
            {{/unless}}
        {{/each}}
    </tr>
{{/each}}

{{#if displayPagination}}
    <div class="pagination">
        {{page-numbers content=pagedContent}}
    </div>
{{/if}}
